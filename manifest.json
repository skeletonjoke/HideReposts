{
    "manifest_version": 2, // fixed value
   
    "name": "Soundcloud hide repost",
    "version": "1.1",
    "description": "Hideand skip reposts on your soundcloud stream.",
    "background": {
      "scripts":["background.js"]
    },
    "content_scripts": [{
      "css": ["style.css"],
      "js": ["main.js"],
      "matches": ["https://soundcloud.com/*"]
    }],
    "permissions": [
      "webNavigation"
    ]
   
  }